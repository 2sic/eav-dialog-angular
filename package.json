{
  "name": "eav-item-dialog-angular",
  "version": "10.00.00",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",

    "//": "build shared parts - always in the same mode for both prod/dev",
    "build-core": "node ./build-helpers/build-core",
    "build-languages": "node ./build-helpers/build-languages",
    "pre-build": "npm run build-core && npm run build-languages",

    "//": "Main system: develop builds",
    "dev-main": "npm run pre-build && ng build",
    "dev-main-watch": "npm run pre-build && ng build --watch",

    "//": "Main system: production builds",
    "prod-main": "npm run set-env-prod && npm run pre-build && ng build --extraWebpackConfig ./webpack.extra.js --prod",
    "prod-main-watch": "npm run pre-build && ng build --extraWebpackConfig ./webpack.extra.js --prod --watch",

    "//": "WYSIWYG builds, dev and prod",
    "dev-wysiwyg": "ng build field-string-wysiwyg --extraWebpackConfig ../../webpack.extra.js",
    "dev-wysiwyg-watch": "ng build field-string-wysiwyg --extraWebpackConfig ../../webpack.extra.js --watch",
    "prod-wysiwyg": "ng build field-string-wysiwyg --extraWebpackConfig ../../webpack.extra.js --prod",

    "//": "Custom GPS builds, dev and prod",
    "dev-custom-gps": "webpack --config ./projects/field-custom-gps/webpack.config.js",
    "dev-custom-gps-watch": "webpack --config ./projects/field-custom-gps/webpack.config.js --watch",
    "prod-custom-gps": "webpack --config ./projects/field-custom-gps/webpack.config.js --mode=production",

    "//": "Copy processes to move content from dist to the main system",
    "copy-to-dnn": "node ./build-helpers/copy-to-dnn",
    "copy-to-dnn-watch": "node ./build-helpers/copy-to-dnn --watch",

    "//": "build many things at the same time",
    "dev-all": "npm run dev-wysiwyg && npm run dev-custom-gps && npm run dev-main && npm run copy-to-dnn",
    "dev-all-watch": "concurrently npm:dev-wysiwyg-watch npm:dev-custom-gps-watch npm:dev-main-watch npm:copy-to-dnn-watch",
    "prod-all": "npm run set-env-prod && npm run prod-wysiwyg && npm run prod-custom-gps && npm run prod-main && npm run copy-to-dnn",
    "prod-all-watch": "npm run prod-wysiwyg && npm run prod-custom-gps && concurrently npm:prod-main-watch npm:copy-to-dnn-watch",

    "//": "WIP! this will set the global environment variable to 'production', or something else.",
    "//": "in production mode, the source-maps will be on another server",
    "set-env-prod": "set NODE_ENV=production",
    "set-env-dev": "set NODE_ENV=dev",

    "//": "other stuff",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "bundle-report": "webpack-bundle-analyzer ../2sxc-dnn742/Website/DesktopModules/ToSIC_SexyContent/dist/ng-edit/stats.json",

  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.1",
    "@angular/cdk": "^7.2.0",
    "@angular/common": "^7.2.1",
    "@angular/compiler": "^7.2.1",
    "@angular/core": "^7.2.1",
    "@angular/elements": "^7.2.1",
    "@angular/flex-layout": "^7.0.0-beta.22",
    "@angular/forms": "^7.2.1",
    "@angular/http": "^7.2.1",
    "@angular/material": "^7.2.0",
    "@angular/platform-browser": "^7.2.1",
    "@angular/platform-browser-dynamic": "^7.2.1",
    "@angular/router": "^7.2.1",
    "@material/layout-grid": "^0.41.0",
    "@ngrx/effects": "^7.0.0",
    "@ngrx/entity": "^7.0.0",
    "@ngrx/store": "^7.0.0",
    "@ngrx/store-devtools": "^7.0.0",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "@tinymce/tinymce-angular": "^2.5.0",
    "@types/google-maps": "^3.2.0",
    "@webcomponents/custom-elements": "^1.2.1",
    "chokidar": "^2.0.4",
    "core-js": "^2.6.1",
    "document-register-element": "^1.8.1",
    "hammerjs": "^2.0.8",
    "ng-pick-datetime": "^7.0.0",
    "ngx-build-plus": "^7.7.5",
    "ngx-drag-and-drop-lists": "^1.2.5",
    "ngx-dropzone-wrapper": "^7.1.0",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.3.3",
    "rxjs-tslint": "^0.1.6",
    "tslib": "^1.9.0",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.28"
  },
  "devDependencies": {
